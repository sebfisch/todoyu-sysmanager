{$tabs}

<table width="650" border="0" cellspacing="0" cellpadding="3">
  <tr>
    <th width="180" scope="col">Extension</th>
    <th width="356" scope="col">Recordtype</th>
    <th width="40" scope="col">#</th>
    <th width="50" scope="col">&nbsp;</th>
  </tr>

	{foreach $extensions extKey extension}
		{foreach $extension.records type record}
			<tr>
				<td>{$extension.title}</td>
				<td>{$record.title}</td>
				<td>{$record.count}</td>
				<td><a href="?ext=admin&mod=extensions&extkey={$extKey}&tab=records&type={$type}">edit link</a></td>
			</tr>
		{/foreach}
	{/foreach}

</table>



