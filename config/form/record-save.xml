<?xml version="1.0" encoding="UTF-8"?>
<form>
	<attributes>
		<attribute name="name">ext</attribute>
	</attributes>

	<fieldsets>

		<fieldset name="save">

			<elements>
				<field type="button" name="save">
					<type>submit</type>
					<text>LLL:form.field.save</text>
					<class>buttonSave</class>
					<hasIcon />
				</field>
				<field type="button" name="cancel">
					<text>LLL:form.field.cancel</text>
					<type>button</type>
					<class>buttonCancel</class>
					<hasIcon />
					<onclick>Todoyu.Ext.sysmanager.Extensions.Records.closeForm('#record-extkey#','#record-type#')</onclick>
				</field>
			</elements>

		</fieldset>

	</fieldsets>
</form>